{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "vr_collection_vaccine_coverage",
  "additionalProperties": false,
  "required": ["date_unix", "vrcode", "percentage", "date_of_insertion_unix"],
  "properties": {
    "date_unix": {
      "type": "integer"
    },
    "vrcode": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "percentage": {
      "type": "number"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    }
  }
}
